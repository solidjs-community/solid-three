{
  "name": "@solid-three/fiber",
  "version": "0.9.0",
  "description": "SolidJS bindings for ThreeJS",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-three.git"
  },
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**",
    "types/**",
    "README.md"
  ],
  "scripts": {
    "test": "vitest",
    "watch": "nodemon  --exec \"pnpm build\"",
    "build": "concurrently \"pnpm build:lib\" \"pnpm build:types\"",
    "build:lib": "vite build",
    "build:types": "tsc --emitDeclarationOnly --declarationDir types"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.4",
    "@types/jest": "^26.0.24",
    "@types/node": "^18.11.18",
    "@types/three": "^0.149.0",
    "@vinxi/tsconfig": "0.0.3",
    "concurrently": "^8.2.0",
    "dts-bundle": "^0.7.3",
    "nodemon": "^3.0.1",
    "solid-js": "^1.7.0",
    "three": "0.149.0",
    "three-stdlib": "^2.25.0",
    "typescript": "^5.1.6",
    "utility-types": "^3.10.0",
    "vite": "4.4.9",
    "vite-plugin-dts": "^3.3.1",
    "vite-plugin-inspect": "0.7.14",
    "vite-plugin-solid": "2.5.0"
  },
  "peerDependencies": {
    "solid-js": "*",
    "three": "*"
  },
  "jest": {
    "preset": "scripts/jest/node"
  },
  "dependencies": {
    "@solid-primitives/memo": "^1.3.4",
    "@solid-primitives/resize-observer": "^2.0.18"
  },
  "packageManager": "pnpm@7.26.0"
}
